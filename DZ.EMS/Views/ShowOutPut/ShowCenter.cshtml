<div style="width:100%; margin:0 auto; background:white;" class="background">
    <div id="tt" class="easyui-tabs" >
        @*<div title="Tab1" style="padding:20px;display:none;">
                tab1
            </div>*@
        @foreach (DZ.MODEL.DZ_MODULE item in @ViewBag.tabs)
        {
            var href = "/" + @item.Controler + "/" + @item.Action;
            <div title="@item.Title" data-options="closable:false,href:'@href'" style="overflow:auto;padding:20px;display:none;">

            </div>
        }

        @*<div title="韵达" data-options="iconCls:'icon-reload',closable:false,href:'/input/yd'" style="padding:20px;display:none;">

            </div>*@
